interface={{ rpi_wifi_ap_WLAN }}
# If you are using Adafruit wifi adapters - uncomment this:
#driver=rtl871xdrv
driver=nl80211
ssid={{ rpi_wifi_ap_essid }}
country_code=US
hw_mode=g
channel=6
macaddr_acl=0
auth_algs=1
ignore_broadcast_ssid=0
wpa=2
wpa_passphrase={{ rpi_wifi_ap_passphrase }}
wpa_key_mgmt=WPA-PSK
wpa_pairwise=CCMP
wpa_group_rekey=86400
ieee80211n=1
wme_enabled=1
